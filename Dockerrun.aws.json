{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "django-app",
      "image": "dreamwars99/django-cicd-app:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [{"hostPort": 8080, "containerPort": 8080}],
      "environment": [
        {"name": "DATABASE_HOST", "value": "your_db_host"},
        {"name": "DATABASE_USER", "value": "your_db_user"},
        {"name": "DATABASE_PASSWORD", "value": "your_db_password"},
        {"name": "SECRET_KEY", "value": "your_secret_key"},
        {"name": "DJANGO_SETTINGS_MODULE", "value": "KB_FinAIssist.settings"}
      ]
    }
  ]
}
