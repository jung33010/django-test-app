  {
    "AWSEBDockerrunVersion": "1",
    "Image": {
      "Name": "your_dockerhub_username/your_dockerhub_image:tag",
      "Update": "true"
    },
    "Ports": [
      {
        "ContainerPort": 8000,
        "HostPort": 8000
      }
    ],
    "Environment": {
      "DATABASE_HOST": "$DATABASE_HOST",
      "DATABASE_USER": "$DATABASE_USER",
      "DATABASE_PASSWORD": "$DATABASE_PASSWORD"
    },
    "Volumes": [],
    "Logging": "/var/log/nginx"
  }